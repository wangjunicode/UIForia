<UITemplate>

    <Style src="SpaceGameDemo/SpaceGameRoot.style"/>

    <Style>

        style parent {
        BackgroundColor = white;
        Padding = 20px;
        [hover] {
        BackgroundColor = blue;
        }
        }

        style child {
        PreferredSize = 100px;
        BackgroundColor = green;
        [hover] {
        BackgroundColor = blue;
        }
        }

        style ignored {
        LayoutBehavior = Ignored;
        }

        style transcluded {
        LayoutBehavior = TranscludeChildren;
        }

        style small {
        PreferredSize = 50px;
        BackgroundColor= Yellow;
        }

        style clipper {
        Overflow = hidden;
        }

        style ignored-child {
        PreferredSize = 10px;
        BackgroundColor = black;
        LayoutFitHorizontal = Fill;
        }

        style rotated {
        TransformRotation = 45;
        }

        style blue {
        BackgroundColor = blue;
        PreferredSize = 150px 100px;
        }

        style a {
        BackgroundColor = red;
        AlignX = 0.75w Screen;
        }

    </Style>
